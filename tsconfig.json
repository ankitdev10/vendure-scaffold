{
    "compilerOptions": {
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "strictPropertyInitialization": false,
        "target": "es2019",
        "strict": true,
        "sourceMap": false,
        "skipLibCheck": true,
        "outDir": "./dist",
        "baseUrl": "./"
    },
    "exclude": ["node_modules", "migration.ts", "src/plugins/**/ui/*", "src/custom-admin-ui/admin-ui"],
     "include": ["src/**/*", "**/*.tsx"],
    "ts-node": {
        "files": true
    }
}
